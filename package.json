{
  "name": "give-me-a-rest",
  "version": "1.0.0",
  "description": "Export your REST api through functions and builders",
  "types": "./dist/cjs/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "bin": {
    "give-me-a-rest": "dist/cjs/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oron-mozes/give-me-a-rest.git"
  },
  "keywords": [
    "REST",
    "Testing"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oron-mozes/give-me-a-rest/issues"
  },
  "homepage": "https://github.com/oron-mozes/give-me-a-rest#readme",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "dependencies": {
    "typescript": "^4.9.5"
  }
}
